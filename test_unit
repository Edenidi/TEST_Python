import unittest

class TestMonProgramme(unittest.TestCase) : 
    def test_nb_caracteres(self,code) :
        self.assertFalse(len(code) < 10 or len(code) > 25)
        self.assertFalse(chr.isdigit() for chr in code == False)
        self.assertFalse(set('[~!@#$%^&*()_+{}":;\']+$').intersection(code) == "")
        self.assertFalse(ele.isupper() for ele in code == False)
        self.assertFalse(code.isupper() == True)

 
